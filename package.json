{
  "name": "react-formik",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/yup": "^0.29.7",
    "axios": "^0.20.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sonarjs": "^0.5.0",
    "formik": "^2.1.5",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-saga": "^1.1.3",
    "tailwindcss": "^1.8.10",
    "typescript": "~3.7.2",
    "yup": "^0.29.3"
  },
  "scripts": {
    "build:css": "postcss src/assets/app.scss -o public/app.css",
    "watch:css": "postcss -w src/assets/app.scss -o public/app.css",
    "start": "concurrently \"yarn watch:css\" \"yarn start-js\"",
    "start-js": "react-scripts start",
    "build": "concurrently \"yarn build:css\" \"yarn build-js\"",
    "build-js": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "lint:fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "pre-commit": "yarn run lint",
    "pre-push": "yarn test",
    "post-merge": "yarn install",
    "post-rewrite": "yarn install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.2.0",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "postcss-cli": "^7.1.1",
    "postcss-nested": "^4.2.1",
    "pretty-quick": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again",
      "post-merge": "yarn install"
    }
  },
  "lint-staged": {
    "*.+(tsx|ts|js|json|css|scss|md)": [
      "prettier --write ."
    ],
    "*.+(ts|js|tsx)": [
      "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    ]
  }
}